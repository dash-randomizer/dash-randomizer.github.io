{
  "name": "dash-rando",
  "version": "1.0.0",
  "description": "Website for the DASH Randomizer.",
  "main": "dash.cli.js",
  "engines": {
    "node": "18.x"
  },
  "type": "module",
  "scripts": {
    "build:headless": "ncc build ./headless/index.ts -o ./headless/dist --minify --no-cache --no-source-map-register",
    "build:web": "node build.js",
    "dev": "npm run build:web -- --watch",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashrando/dashrando.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/dashrando/dashrando.github.io/issues"
  },
  "homepage": "https://dashrando.net",
  "devDependencies": {
    "@types/node": "^18.15.10",
    "@vercel/ncc": "^0.36.1",
    "esbuild": "^0.17.11",
    "esbuild-serve": "^1.0.1",
    "glob": "^9.3.2",
    "prettier": "2.8.7",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "idb-keyval": "^6.2.0"
  }
}
