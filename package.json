{
  "name": "dash-rando",
  "version": "1.0.0",
  "description": "Website for the DASH Randomizer.",
  "main": "dash.cli.js",
  "engines": {
    "node": "18.x"
  },
  "type": "module",
  "scripts": {
    "build:headless": "esbuild headless/index.js --bundle --outfile=headless/dist/dash.headless.js --minify --target=node18 --platform=node",
    "build:web": "node build.js",
    "dev": "npm run build:web -- --watch",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashrando/dashrando.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/dashrando/dashrando.github.io/issues"
  },
  "homepage": "https://dashrando.net",
  "devDependencies": {
    "esbuild": "^0.17.11",
    "esbuild-serve": "^1.0.1",
    "glob": "^9.3.2",
    "prettier": "2.8.7"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "idb-keyval": "^6.2.0"
  }
}
